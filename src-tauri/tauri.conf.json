{
	"$schema": "https://schema.tauri.app/config/2",
	"productName": "TypeTeX",
	"version": "0.1.0",
	"identifier": "com.duchiru.typetex",
	"build": {
		"beforeDevCommand": "bun run ui:dev",
		"devUrl": "http://localhost:1420",
		"beforeBuildCommand": "bun run ui:build",
		"frontendDist": "../build"
	},
	"app": {
		"security": {
			"pattern": {
				"use": "brownfield"
			}
		}
	},
	"bundle": {
		"active": true,
		"targets": "all",
		"icon": [
			"icons/32x32.png",
			"icons/128x128.png",
			"icons/128x128@2x.png",
			"icons/icon.icns",
			"icons/icon.ico"
		]
	}
}
